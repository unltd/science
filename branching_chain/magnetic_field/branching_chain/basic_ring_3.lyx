\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\lang russian
\begin_inset Formula 
\begin{eqnarray*}
\varphi_{1,1}(x)=\gamma_{1}\cdot\varphi_{-1,1}(x), & x\in[0,\pi],\\
\varphi_{2,1}(x)=\gamma_{2}\cdot\varphi_{-1,1}(x), & x\in[0,\pi],
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard

\lang russian
