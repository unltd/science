\end_layout

\begin_layout Standard

\lang russian
\begin_inset Formula 
\begin{eqnarray*}
 &  & \psi_{1,1}(0)=\gamma_{1}\cdot\psi_{-1,1}(0)=\gamma_{1}\cdot u\\
 &  & \psi_{2,1}(0)=\gamma_{2}\cdot\psi_{-1,1}(0)=\gamma_{2}\cdot u\\
 &  & \alpha\cdot\psi_{1,1}-i\hat{p}\psi{}_{1,1}(0)-i\hat{p}\varphi{}_{1,1}(0)=\gamma_{1}(\alpha\cdot\psi_{-1,1}-i\hat{p}\psi{}_{-1,1}(0)-i\hat{p}\varphi_{-1,1}\left(0\right))=\gamma_{1}\cdot v\\
 &  & \alpha\cdot\psi_{2,1}-i\hat{p}\psi{}_{2,1}(0)-i\hat{p}\varphi{}_{2,1}(0)=\gamma_{2}(\alpha\cdot\psi_{-1,1}-i\hat{p}\psi{}_{-1,1}(0)-i\hat{p}\varphi_{-1,1}\left(0\right))=\gamma_{2}\cdot v
\end{eqnarray*}

\end_inset


\end_layout

